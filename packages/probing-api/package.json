{
  "name": "pagopa-interop-probing-api",
  "version": "1.0.0",
  "description": "PagoPA probing api",
  "main": "dist",
  "type": "module",
  "scripts": {
    "test": "vitest --config ./test/vitest.config.ts",
    "test:it": "vitest db --config ./test/vitest.config.ts",
    "lint": "eslint . --ext .ts,.tsx -c .eslintrc",
    "lint:autofix": "eslint . --ext .ts,.tsx --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "node --watch --no-warnings --loader ts-node/esm -r 'dotenv-flow/config' ./src/index.ts",
    "build": "tsc",
    "generate-model": "mkdir -p ./src/model/generated && pnpm openapi-zod-client './open-api/api-external-interop-v1.yaml' -o './src/model/generated/api.ts'",
    "clean-generated": "pnpm exec rm ./src/model/generated/api.ts"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/dotenv-flow": "3.3.3",
    "@types/express": "4.17.21",
    "@types/node": "20.10.4",
    "@types/supertest": "6.0.2",
    "@types/uuid": "9.0.7",
    "supertest": "6.3.4",
    "ts-node": "10.9.2",
    "typescript": "5.2.2",
    "vitest": "0.33.0"
  },
  "dependencies": {
    "@tsconfig/node-lts": "20.1.1",
    "@zodios/core": "10.9.6",
    "@zodios/express": "10.6.1",
    "aws-sdk": "2.1593.0",
    "axios": "1.7.4",
    "cors": "2.8.5",
    "dotenv-flow": "3.3.0",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "openapi-zod-client": "1.13.4",
    "pagopa-interop-probing-commons": "workspace:*",
    "pagopa-interop-probing-eservice-operations-client": "workspace:*",
    "pagopa-interop-probing-models": "workspace:*",
    "ts-pattern": "5.0.6",
    "uuid": "9.0.1",
    "zod": "3.22.4"
  }
}
