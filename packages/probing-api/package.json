{
  "name": "pagopa-interop-probing-api",
  "version": "1.0.0",
  "description": "PagoPA probing api",
  "main": "dist",
  "type": "module",
  "scripts": {
    "test": "vitest --config ./test/vitest.config.ts",
    "test:api": "vitest run --config ./test/vitest.api.config.ts",
    "lint": "eslint .",
    "lint:autofix": "eslint . --fix",
    "format:check": "prettier --check src",
    "format:write": "prettier --write src",
    "start": "tsx -r 'dotenv-flow/config' --watch ./src/index.ts",
    "build": "tsc",
    "check": "tsc --project tsconfig.check.json",
    "generate-model": "mkdir -p ./src/model/generated && pnpm openapi-zod-client './open-api/api-external-interop-v1.yaml' -o './src/model/generated/api.ts'",
    "clean-generated": "pnpm exec rm ./src/model/generated/api.ts"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@probing/vitest-config": "workspace:*",
    "@types/cors": "2.8.17",
    "@types/dotenv-flow": "3.3.3",
    "@types/express": "4.17.21",
    "@types/node": "20.10.4",
    "@types/supertest": "6.0.2",
    "@types/uuid": "9.0.8",
    "globals": "15.14.0",
    "openapi-zod-client": "1.13.4",
    "supertest": "7.1.4",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "tsx": "4.20.5"
  },
  "dependencies": {
    "@zodios/core": "10.9.6",
    "@zodios/express": "10.6.1",
    "axios": "1.12.0",
    "cors": "2.8.5",
    "dotenv-flow": "4.1.0",
    "express": "4.21.2",
    "helmet": "7.1.0",
    "pagopa-interop-probing-commons": "workspace:*",
    "pagopa-interop-probing-eservice-operations-client": "workspace:*",
    "pagopa-interop-probing-models": "workspace:*",
    "ts-pattern": "5.1.2",
    "uuid": "13.0.0",
    "zod": "3.23.8"
  }
}